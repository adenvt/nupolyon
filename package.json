{
  "name": "nupolyon",
  "version": "0.2.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adenvt/nupolyon.git"
  },
  "packageManager": "yarn@3.6.3",
  "description": "Auto-Inject polyfill",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run unit && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.6.5",
    "browserslist-to-esbuild": "^2.0.0",
    "polyfill-library": "^4.8.0",
    "polyfillist": "^0.1.1",
    "ufo": "^1.2.0"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.8.3",
    "@nuxt/schema": "^3.6.5",
    "@nuxt/test-utils": "^3.6.5",
    "@types/polyfill-library": "^3.108.1",
    "changelogen": "^0.5.4",
    "cheerio": "^1.0.0-rc.12",
    "eslint": "^8.46.0",
    "nuxt": "^3.6.5",
    "vitest": "^0.34.1"
  }
}
