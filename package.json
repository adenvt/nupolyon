{
  "name": "nupolyon",
  "version": "0.1.0",
  "description": "Auto-Inject polyfill from polyfill.io",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run unit && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.1.2",
    "browserslist-to-esbuild": "^1.2.0",
    "polyfill-library": "^4.6.0",
    "polyfillist": "^0.1.0",
    "ufo": "^1.0.1"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.1.1",
    "@nuxt/module-builder": "^0.3.0",
    "@nuxt/schema": "^3.1.2",
    "@nuxt/test-utils": "^3.1.2",
    "@types/polyfill-library": "^3.108.1",
    "changelogen": "^0.5.0",
    "eslint": "^8.33.0",
    "nuxt": "^3.1.2",
    "vitest": "^0.30.0"
  }
}
